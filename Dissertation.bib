keywords = {charging optimization,frequency regulation,fuzzy logic,plug-in electric vehicles network,real-time power flow management,renewable energy resources scheduling},
pages = {1197--1202},
publisher = {IEEE},
title = {{Real-time plug-in electric vehicles charging control for V2G frequency regulation}},
year = {2013}
}
