doi = {10.1016/j.microrel.2012.11.010},
file = {:C$\backslash$:/Users/angus/OneDrive/Documents/Year 4 notes/Diss/EB/Literature/State of charge estimation for electric vehicle batteries using unscented.pdf:pdf},
issn = {00262714},
journal = {Microelectronics Reliability},
number = {6},
pages = {840--847},
publisher = {Elsevier Ltd},
title = {{State of charge estimation for electric vehicle batteries using unscented kalman filtering}},
url = {http://dx.doi.org/10.1016/j.microrel.2012.11.010},
volume = {53},
year = {2013}
